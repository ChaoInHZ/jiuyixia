<view class="container">
  <view class="page-body">
    <view class="page-section weui-panel name-card">
      <view class="user-img">
        <image src="/images/user-unlogin.png" mode="aspectFit"></image>
        <view class="level">LV <text>1</text></view>
      </view>
      <view class="user-name">
        <navigator>
          <text>User Name</text>
        </navigator>
      </view>
      <view class="check-in">
        <navigator>
          <image src="/images/check-in.svg" mode="aspectFit"></image>
          <text>签到</text>
        </navigator>
      </view>
      <view class="go-to-edit">
        <navigator>
          <image mode="aspectFit" src="/images/arrow-right-white.svg"></image>
        </navigator>
      </view>

    </view>
    <view class="page-section weui-panel order-panel">
      <view class="weui-cells weui-cells_after-title">
          <navigator url="/pages/order-list/order-list" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd"><text>我的订单</text></view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="/pages/favor/favor" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd"><text>我的收藏</text></view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
      </view>
    </view>

    <view class="page-section weui-panel msg-panel">
      <view class="weui-cells weui-cells_after-title">
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd"><text>我的消息</text></view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd"><text>积分商城</text></view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
      </view>
    </view>
    <view class="page-section weui-panel contact-panel">
      <view class="weui-cells weui-cells_after-title">
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd"><text>预约回收</text></view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator bindtap="dialOut" data-tel="13000000000" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd"><text>客服热线</text></view>
              <view class="weui-cell__ft weui-cell__ft_in-access">13000000000</view>
          </navigator>
      </view>
    </view>

		<view class="weui-tab bottom-nav-bar">
			<view class="weui-tab__panel">
			</view>
			<view class="weui-tabbar">
				<view class="weui-tabbar__item nav-homepage">
        <navigator url="/pages/index/index">
					<view class="icon-img">
            <image src="/images/nav-home.svg"></image>
            </view>
          <text class="weui-tabbar__label">首页</text>
        </navigator>
				</view>
				<view class="weui-tabbar__item nav-add">
        <navigator url="/pages/sales-pre/sales-pre">
					<view class="icon-img">
            <text>+</text>
          </view>
          <text class="weui-tabbar__label">我要卖</text>
        </navigator>
				</view>
				<view class="weui-tabbar__item weui-bar__item_on nav-mine">
        <navigator url="/pages/user-center/user-center">
					<view class="icon-img">
            <image src="/images/nav-user-active.svg"></image>
          </view>
					<text class="weui-tabbar__label">我的</text>
        </navigator>
				</view>
			</view>
		</view>
  </view>
</view>